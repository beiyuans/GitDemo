
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <script src="~/Scripts/jquery-1.8.2.js"></script>
  <script>
      $(function () {
          $.ajax(
              {
                  type: "get",
                  url: "js",
                  success: function (data) {
                      $("#uid").val(data.id);
                      $("#uname").val(data.name);
                  }
              })
      })
  </script>
      
      
</head>
<body>
    <div> 
        <p>这是index页面</p>
        编号：<input type="text" id="uid" />
        名称：<input type="text" id="uname" />
    </div>
</body>
</html>
